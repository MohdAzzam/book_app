<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/styles/style.css">
</head>
<body>
    <header>
        <nav>
            <h1 class="header-nav">Best Books!</h1>
            <button class="button1">click here!</button>
        </nav>
    </header>
    <div class="co">
        <h4 class="new-header">Search by Title or Author</h4>
    </div>
    <main class="container-show">
        <% searchResults.forEach( book => { %>
            <img src="<%=book.img%>" >
            <h3><%= book.title %></h3>
            <p><%= book.description %></p>
        <% }) %>
    </main>
    <footer>
      <div></div>
      <div></div>
      <div></div>
      <div></div>
  </footer>
  </body>
</html>