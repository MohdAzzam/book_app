<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Book Collection</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <main>
        <% searchResults.forEach( book => { %>
            <img src="<%=book.img%>" >
            <h3><%= book.title %></h3>
            <p><%= book.description %></p>
        <% }) %>
    </main>
  </body>
</html>